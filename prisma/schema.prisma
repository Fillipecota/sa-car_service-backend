generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  name      String
  email     String   @unique
  password  String
  createdAt DateTime
  updatedAt DateTime @updatedAt

  @@map("users")
}

model Product {
  id        Int      @id @default(autoincrement())
  categoria String
  modelo    String
  cor       String
  motor     Float
  pneu      String
  ano       Int
  createdAt DateTime

  @@map("products")
}
